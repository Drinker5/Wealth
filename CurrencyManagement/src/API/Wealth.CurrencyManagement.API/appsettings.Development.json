{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "UsePostgres": true,
  "EventBus": {
    "ConnectionFactory": {
      "HostName": "localhost"
    }
  },
  "ConnectionStrings": {
    "Wealth.CurrencyManagement": "Host=127.0.0.1;Username=postgres;Password=postgres;Persist Security Info=True;Database=Wealth;"
  },
  "OutboxPolling": {
    "Enabled": true,
    "RetryCount": 5
  },
  "Serilog": {
    "Using": [
      "Serilog.Sinks.Console"
    ],
    "MinimumLevel": {
      "Default": "Verbose",
      "Override": {
        "Microsoft": "Warning",
        "System": "Warning"
      }
    },
    "WriteTo": [
      {
        "Name": "Console",
        "Args": {
          "outputTemplate": "{Timestamp:HH:mm:ss} [{Level:u3}] {Message:lj}{NewLine}{Exception}"
        }
      }
    ],
    "Enrich": [
      "FromLogContext"
    ]
  }
}
